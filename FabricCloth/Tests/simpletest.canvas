operator applyTurbulizeVec3_task<<<index>>>(Vec3 p[], io Vec3 v[], Integer fixed[], Vec3 c, Float32 a, Vec3 sf)
{
  if ( !fixed[index] ) {
    Vec3 q = c + sf * p[index];
    v[index] += Vec3(
      0.1 * a * perlinNoise(q.x, q.z, q.y),
      0.1 * a * perlinNoise(q.y, q.x, q.z),
      0.1 * a * perlinNoise(q.z, q.y, q.x)
      );
  }
}

